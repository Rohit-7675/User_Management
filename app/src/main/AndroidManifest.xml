<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rppatil.inceptivetech">

    <application
        android:allowBackup="true"
        android:icon="@drawable/circle_appicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/circle_appicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RegistrationActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" android:theme="@style/AppTheme.NoActionBar" android:screenOrientation="portrait"/>
        <activity android:name=".ContactUs" android:launchMode="singleTask" android:screenOrientation="portrait"/>
        <activity android:name=".AboutUs" android:launchMode="singleTask" android:screenOrientation="portrait"/>
    </application>
</manifest>